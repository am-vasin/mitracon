*
* Удаление чужих списков
*
PROCEDURE Del_Lst
PARAMETER mt_

USE (base_path+"LIST_DET") ORDER TAG LIST
SELE 0
USE (base_path+"LIST_TIT")
SCAN
  IF mt_ .AND. FIRM # "TEL"
    LOOP
  ENDIF
  IF .NOT. mt_ .AND. FIRM = "TEL"
    LOOP
  ENDIF
  SELECT LIST_DET
  SEEK STR(LIST_TIT.LST_NUM, 6)
  DELETE REST WHILE LIST_NUM = STR(LIST_TIT.LST_NUM, 6)
  SELECT LIST_TIT
  DELETE
ENDSCAN

CLOSE DATA
