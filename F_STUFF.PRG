PROCEDURE F_Stuff
PARAMETERS f_name, src, res

PRIVATE pos, nf

pos = F_Seek(f_name, src)
IF pos = -1
  RETURN -1
ENDIF

nf = FOPEN(f_name, 2)
= FSEEK(nf,pos)
= FWRITE(nf,res)
= FCLOSE(nf)

RETURN 0
