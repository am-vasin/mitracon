PROCEDURE Round_6
PARAMETER v, nds_w
PRIVATE w

IF TYPE("nds_w") = "N"
  w = ROUND(v*100/(nds_w+100), 2)
  w = w*(1+nds_w/100)
ELSE
  w = ROUND(v*100/(nds_flt+100), 2)
  w = w*(1+nds_flt/100)
ENDIF

RETURN ROUND(w,5)
