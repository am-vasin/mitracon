USE DIFF ORDER TAG CODE
SELECT 0
USE D:\TTT\STO_S
CLEAR
? RECCOUNT()
NN = 0
DD = 0
SCAN
  NN = NN+1
  IF NN % 1000 = 0
    WAIT WIND STR(NN) NOWAIT
  ENDIF
  IF QNT = 0
    LOOP
  ENDIF
  SELECT DIFF
  IF .NOT. SEEK(STO_S.CODE)
    APPEND BLANK
    REPLACE CODE WITH STO_S.CODE
  ENDIF
  REPLACE COMPEL WITH STO_S.QNT
  IF COMPEL # QNT_REP
    DD = DD+1
  ENDIF
  SELECT STO_S
ENDSCAN
? DD
CLOSE DATA
