USE DIFF ORDER TAG CODE
SELECT 0
USE D:\TTT\ITEMS
CLEAR
? RECCOUNT()
NN = 0
DD = 0
SCAN
  NN = NN+1
  IF NN % 1000 = 0
    WAIT WIND STR(NN) NOWAIT
  ENDIF
  IF QNT = 0
    LOOP
  ENDIF
  SELECT DIFF
  IF .NOT. SEEK(ITEMS.CODE)
    APPEND BLANK
    REPLACE CODE WITH ITEMS.CODE
  ENDIF
  REPLACE SAIT WITH ITEMS.QNT
  IF SAIT # QNT_REP
    DD = DD+1
  ENDIF
  SELECT ITEMS
ENDSCAN
? DD
CLOSE DATA
