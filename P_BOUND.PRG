PROCEDURE P_Bound
PARAMETERS parm
EXTERNAL ARRAY prc_00
PRIVATE i, i_sav
IF TYPE ("prc_00(1,1)") # "N" 
  RETURN " "
ENDIF

i_sav = 1
FOR i = 1 TO ALEN(prc_00,1)  
  IF parm <= prc_00(i,1)
    EXIT
  ENDIF
  i_sav = i
ENDFOR
RETURN prc_00(i,2)    
